{
  "name": "i18n-key-sync",
  "version": "1.0.0",
  "description": "CLI and library to merge and sync missing keys across i18n JSON files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "i18n-key-sync": "./dist/cli/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "nyc --node-option=loader=ts-node/esm mocha",
    "coverage": "nyc report --reporter=lcov --reporter=text-summary"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "i18n",
    "cli",
    "json",
    "localization",
    "translation",
    "sync"
  ],
  "author": "fahadmustjab",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.0",
    "@types/sinon": "^17.0.4",
    "@types/yargs": "^17.0.33",
    "chai": "^5.2.1",
    "mocha": "^11.7.1",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.0"
  }
}
