{
  "name": "i18n-key-sync",
  "version": "0.0.0-development",
  "description": "CLI and library to merge and sync missing keys across i18n JSON files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "i18n-key-sync": "dist/cli/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fahadmustjab/i18n-key-sync.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "yarn build",
    "semantic-release": "semantic-release",
    "test": "mocha -r ts-node/register/transpile-only --extension ts tests/**/*.spec.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "i18n",
    "cli",
    "json",
    "localization",
    "translation",
    "sync"
  ],
  "author": "fahadmustjab",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.0",
    "@types/sinon": "^17.0.4",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "chai": "^5.2.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "mocha": "^11.7.1",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.39.0"
  }
}
